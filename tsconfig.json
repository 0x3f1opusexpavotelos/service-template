{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "sourceMap": true,
      "stripInternal": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      // strict: true
      "strictBindCallApply": true,
      "strictNullChecks": true,
      "strictPropertyInitialization": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
    },
    "include": [
      "src/**/*.ts",
    ],
    "exclude": [
      "node_modules"
    ]
  }